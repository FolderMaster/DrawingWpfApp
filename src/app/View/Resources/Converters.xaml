                    xmlns:converters="clr-namespace:View.Converters">
    <converters:ByteToBrushConverter x:Key="ByteToBrushConverter"/>
    <converters:AlphaToColorConverter x:Key="AlphaToColorConverter"/>
    <converters:ColorToBrushConverter x:Key="ColorToBrushConverter"/>
